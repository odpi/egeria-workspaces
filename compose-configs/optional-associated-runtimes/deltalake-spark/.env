# MinIO credentials and default bucket
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
S3_BUCKET=datalake

# MinIO host port mappings (avoid conflicts with other stacks)
MINIO_HOST_PORT_API=8200
MINIO_HOST_PORT_CONSOLE=8201

# Spark worker sizing (adjust to your machine)
SPARK_WORKER_CORES=2
SPARK_WORKER_MEMORY=2G
